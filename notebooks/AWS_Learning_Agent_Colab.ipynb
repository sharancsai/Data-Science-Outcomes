        "    print(f\"üë§ You: {message}\")\n",
        "    print(f\"ü§ñ AWS Assistant: {response}\")\n",
        "    print(\"-\" * 50)\n",
        "    \n",
        "    return response\n",
        "\n",
        "# Example usage\n",
        "print(\"üéØ Let's start learning AWS! Try the examples below or ask your own questions.\\n\")\n",
        "\n",
        "# Example questions\n",
        "example_questions = [\n",
        "    \"What is Amazon EC2?\",\n",
        "    \"How do I create an S3 bucket?\", \n",
        "    \"What is a VPC?\",\n",
        "    \"Explain AWS Lambda\",\n",
        "    \"What are the benefits of cloud computing?\"\n",
        "]\n",
        "\n",
        "print(\"üìù Example questions you can ask:\")\n",
        "for i, q in enumerate(example_questions, 1):\n",
        "    print(f\"{i}. {q}\")\n",
