        "        self.progress_data['topics'][topic] = score\n",
        "        self.save_progress()\n",
        "    \n",
        "    def complete_lab(self, lab_name):\n",
        "        if lab_name not in self.progress_data['labs_completed']:\n",
        "            self.progress_data['labs_completed'].append(lab_name)\n",
        "            self.save_progress()\n",
        "            print(f\"üéâ Congratulations! You completed the {lab_name} lab!\")\n",
        "    \n",
        "    def increment_questions(self):\n",
        "        self.progress_data['total_questions'] += 1\n",
        "        self.save_progress()\n",
        "    \n",
        "    def show_progress(self):\n",
        "        print(\"üìä Your AWS Learning Progress\")\n",
        "        print(\"=\" * 40)\n",
        "        \n",
        "        # Calculate days learning\n",
        "        start_date = datetime.fromisoformat(self.progress_data['start_date'])\n",
