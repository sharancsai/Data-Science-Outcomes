    {
      "cell_type": "code",
      "execution_count": null,
      "metadata": {
        "id": "create-agent-files"
      },
      "outputs": [],
      "source": [
        "# Create simplified agent implementation for Colab\n",
        "agent_content = '''\n",
        "import json\n",
        "import logging\n",
        "from datetime import datetime\n",
        "from typing import Dict, List, Any\n",
        "from transformers import pipeline, AutoTokenizer, AutoModelForCausalLM\n",
        "import torch\n",
        "\n",
        "logging.basicConfig(level=logging.INFO)\n",
        "logger = logging.getLogger(__name__)\n",
        "\n",
        "class ColabAWSAgent:\n",
        "    \"\"\"Simplified AWS Learning Agent for Google Colab.\"\"\"\n",
        "    \n",
        "    def __init__(self):\n",
        "        self.model_name = \"microsoft/DialoGPT-medium\"\n",
        "        self.tokenizer = None\n",
        "        self.model = None\n",
        "        self.chat_history = []\n",
        "        self.aws_knowledge = self._load_aws_knowledge()\n",
        "        self._initialize_model()\n",
        "        \n",
        "    def _initialize_model(self):\n",
        "        \"\"\"Initialize the conversational model.\"\"\"\n",
        "        try:\n",
        "            print(\"Loading AI model... This may take a few minutes.\")\n",
        "            self.tokenizer = AutoTokenizer.from_pretrained(self.model_name)\n",
        "            self.model = AutoModelForCausalLM.from_pretrained(self.model_name)\n",
        "            \n",
        "            if self.tokenizer.pad_token is None:\n",
        "                self.tokenizer.pad_token = self.tokenizer.eos_token\n",
        "                \n",
        "            print(\"‚úÖ AI model loaded successfully!\")\n",
        "        except Exception as e:\n",
        "            print(f\"Error loading model: {e}\")\n",
        "            self.model = None\n",
        "    \n",
        "    def _load_aws_knowledge(self):\n",
        "        \"\"\"Load AWS knowledge base.\"\"\"\n",
        "        return {\n",
        "            \"ec2\": \"Amazon EC2 provides scalable virtual servers in the cloud. You can launch instances, configure security groups, and manage your compute resources.\",\n",
        "            \"s3\": \"Amazon S3 is object storage built to store and retrieve any amount of data. It\\'s highly durable and scalable.\",\n",
        "            \"vpc\": \"Amazon VPC lets you create a virtual network in the AWS cloud, with your own IP address range, subnets, and routing tables.\",\n",
        "            \"lambda\": \"AWS Lambda is a serverless compute service that runs your code in response to events automatically.\",\n",
        "            \"rds\": \"Amazon RDS makes it easy to set up, operate, and scale a relational database in the cloud.\"\n",
        "        }\n",
        "    \n",
        "    def get_aws_info(self, query):\n",
        "        \"\"\"Get AWS service information.\"\"\"\n",
        "        query_lower = query.lower()\n",
        "        for service, info in self.aws_knowledge.items():\n",
        "            if service in query_lower:\n",
        "                return f\"**{service.upper()}**: {info}\"\n",
        "        \n",
        "        # Return general AWS info if no specific service found\n",
        "        return \"AWS offers many services including EC2 (compute), S3 (storage), VPC (networking), Lambda (serverless), and RDS (database). What specific service would you like to learn about?\"\n",
        "    \n",
        "    def generate_response(self, user_input):\n",
        "        \"\"\"Generate response to user input.\"\"\"\n",
        "        try:\n",
        "            # Check if asking about specific AWS service\n",
        "            aws_response = self.get_aws_info(user_input)\n",
        "            if any(service in user_input.lower() for service in self.aws_knowledge.keys()):\n",
        "                return aws_response\n",
        "            \n",
        "            # Use the conversational model for general questions\n",
        "            if self.model is not None:\n",
        "                # Prepare context with AWS focus\n",
        "                context = \"You are an AWS learning assistant. Help students learn about Amazon Web Services. \"\n",
        "                full_input = context + user_input\n",
        "                \n",
        "                # Tokenize and generate\n",
        "                inputs = self.tokenizer.encode(full_input, return_tensors=\"pt\", max_length=512, truncation=True)\n",
        "                \n",
        "                with torch.no_grad():\n",
        "                    outputs = self.model.generate(\n",
        "                        inputs, \n",
        "                        max_length=inputs.shape[1] + 100,\n",
        "                        num_return_sequences=1,\n",
        "                        temperature=0.7,\n",
        "                        pad_token_id=self.tokenizer.pad_token_id,\n",
        "                        do_sample=True\n",
        "                    )\n",
        "                \n",
        "                response = self.tokenizer.decode(outputs[0], skip_special_tokens=True)\n",
        "                # Extract just the generated part\n",
        "                response = response[len(full_input):].strip()\n",
        "                \n",
        "                if response:\n",
        "                    return response\n",
        "            \n",
        "            # Fallback responses\n",
        "            fallback_responses = [\n",
        "                \"I\\'m here to help you learn AWS! Ask me about EC2, S3, VPC, Lambda, or any other AWS service.\",\n",
        "                \"Great question about AWS! Let me help you understand that better.\",\n",
        "                \"AWS has many powerful services. Which one would you like to explore?\"\n",
        "            ]\n",
        "            \n",
        "            import random\n",
        "            return random.choice(fallback_responses)\n",
        "            \n",
        "        except Exception as e:\n",
        "            logger.error(f\"Error generating response: {e}\")\n",
        "            return \"I\\'m sorry, I encountered an issue. Please try asking about a specific AWS service like EC2, S3, or VPC!\"\n",
        "    \n",
        "    def chat(self, user_input):\n",
        "        \"\"\"Main chat interface.\"\"\"\n",
        "        response = self.generate_response(user_input)\n",
        "        \n",
        "        # Store in chat history\n",
        "        self.chat_history.append({\n",
        "            \"user\": user_input,\n",
        "            \"assistant\": response,\n",
        "            \"timestamp\": datetime.now().isoformat()\n",
        "        })\n",
        "        \n",
        "        return response\n",
        "\n",
        "# Global agent instance\n",
        "agent = None\n",
        "\n",
        "def get_agent():\n",
        "    global agent\n",
        "    if agent is None:\n",
        "        agent = ColabAWSAgent()\n",
        "    return agent\n",
        "'''\n",
        "\n",
        "with open('agent/colab_agent.py', 'w') as f:\n",
        "    f.write(agent_content)\n",
        "\n",
        "print(\"‚úÖ Agent implementation created!\")"
      ]
    },
