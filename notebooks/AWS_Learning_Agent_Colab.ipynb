        "        \n",
        "        print(f\"üìÖ Days learning: {days_learning}\")\n",
        "        print(f\"‚ùì Total questions asked: {self.progress_data['total_questions']}\")\n",
        "        print(f\"üß™ Labs completed: {len(self.progress_data['labs_completed'])}\")\n",
        "        \n",
        "        if self.progress_data['labs_completed']:\n",
        "            print(\"   Completed labs:\")\n",
        "            for lab in self.progress_data['labs_completed']:\n",
        "                print(f\"     ‚úÖ {lab}\")\n",
        "        \n",
        "        if self.progress_data['topics']:\n",
        "            print(\"\\nüéØ Topic Knowledge:\")\n",
        "            for topic, score in self.progress_data['topics'].items():\n",
        "                print(f\"     {topic}: {score}%\")\n",
        "        \n",
        "        # Progress suggestions\n",
        "        print(\"\\nüí° Next Steps:\")\n",
