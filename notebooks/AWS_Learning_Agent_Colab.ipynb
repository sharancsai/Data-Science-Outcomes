      "source": [
        "# Advanced interactive chat with input widget\n",
        "import ipywidgets as widgets\n",
        "from IPython.display import display, clear_output\n",
        "\n",
        "def create_chat_interface():\n",
        "    \"\"\"Create an interactive chat interface.\"\"\"\n",
        "    \n",
        "    # Create widgets\n",
        "    input_box = widgets.Text(\n",
        "        placeholder='Ask me anything about AWS...',\n",
        "        description='Question:',\n",
        "        style={'description_width': 'initial'},\n",
        "        layout=widgets.Layout(width='80%')\n",
        "    )\n",
        "    \n",
        "    send_button = widgets.Button(\n",
        "        description='Send',\n",
        "        button_style='primary',\n",
        "        icon='paper-plane'\n",
        "    )\n",
        "    \n",
        "    output_area = widgets.Output()\n",
        "    \n",
        "    def send_message(b):\n",
        "        \"\"\"Handle send button click.\"\"\"\n",
        "        with output_area:\n",
        "            if input_box.value.strip():\n",
        "                response = chat_with_agent(input_box.value)\n",
        "                input_box.value = ''  # Clear input\n",
        "            else:\n",
        "                print(\"Please enter a question!\")\n",
        "    \n",
        "    def handle_enter(sender):\n",
        "        \"\"\"Handle enter key press.\"\"\"\n",
        "        send_message(None)\n",
        "    \n",
        "    # Connect event handlers\n",
        "    send_button.on_click(send_message)\n",
        "    input_box.on_submit(handle_enter)\n",
        "    \n",
        "    # Layout\n",
        "    input_row = widgets.HBox([input_box, send_button])\n",
        "    chat_interface = widgets.VBox([input_row, output_area])\n",
        "    \n",
        "    return chat_interface\n",
        "\n",
        "# Create and display the chat interface\n",
        "print(\"üöÄ Advanced Chat Interface - Type your questions below!\")\n",
        "chat_ui = create_chat_interface()\n",
        "display(chat_ui)"
      ]
